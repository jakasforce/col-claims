    <description>Flujo disparado antes de la actualización de una cita de servicio.</description>
    <environments>Default</environments>
    <formulas>
        <name>actualDurationInMinutes</name>
        <dataType>Number</dataType>
        <expression>IF(
   OR(
      ISBLANK({!$Record.SchedEndTime}),
      ISBLANK({!$Record.SchedStartTime})
   ),
   0,
   ({!$Record.ActualEndTime}-{!$Record.ActualStartTime})*24*60
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>currentDurationInMinutes</name>
        <dataType>Number</dataType>
        <expression>IF(
   OR(
        ISBLANK({!$Record.SchedEndTime}),
        ISBLANK({!$Record.SchedStartTime})
    ),
   0,
   ({!$Record.SchedEndTime}-{!$Record.SchedStartTime})*24*60
)</expression>
        <scale>0</scale>
    </formulas>
