public with sharing class CS_FinalDateWorkingDaysCalculator_Inv {
	@InvocableMethod
	public static List<Results> execute(List<Requests> requestList) {
		//* Wrap the Results object in a List container
		//* (an extra step added to allow this interface to also support bulkification)
		List<Results> responseWrapper = new List<Results>();
		for (Requests request : requestList) {
			Date finalDate = CS_Utilities.getDateAfterWorkingDays(request.initialDate, request.daysToAdd);
			//* Create a Results object to hold the return values
			Results response = new Results();
			//* Add the return values to the Results object
			response.finalDate = finalDate;
			responseWrapper.add(response);
		}
		return responseWrapper;
	}

	public class Requests {
		//* Add input fields of the invocable action (ONLY FOR MULTIPLE INCOMES).
		@InvocableVariable(label='Initial Date' description='Initial Date since we calculate the Final Date.' required=true)
		public Date initialDate;
		@InvocableVariable(label='Days to add' description='Working days that we expect to add.' required=true)
		public Integer daysToAdd;
	}

	public class Results {
		//* Add output fields of the invocable action (ONLY FOR MULTIPLE OUTCOMES).
		@InvocableVariable(
			label='Final Date'
			description='Final calculated date. It represents the Initial Date plus n (Days to add input) working days.'
			required=true
		)
		public Date finalDate;
	}
    
