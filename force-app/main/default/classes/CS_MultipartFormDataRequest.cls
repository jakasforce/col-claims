public class CS_MultipartFormDataRequest {
	public class MultipartRequestException extends Exception {
		//* No ecribir nada
	}

	public class CS_MultipartParameter {
		public String key { get; private set; }
		public String value { get; private set; }
		public CS_MultipartParameter(String key, String value) {
			this.key = key;
			this.value = value;
		}

		public String getString(String boundary) {
			return '\r\nContent-Disposition: form-data; name=\"' + key + '\"; \r\n\r\n' + value + '\r\n--' + boundary;
		}
	}

	public class CS_MultipartFile {
		public String key { get; private set; }
		public Blob fileBlob { get; private set; }
		public String fileName { get; private set; }
		public String fileExtension { get; private set; }
		public CS_MultipartFile(String key, Blob fileBlob, String fileName, String fileExtension) {
			this.key = key;
			this.fileBlob = fileBlob;
			this.fileName = fileName;
			this.fileExtension = fileExtension;
		}

		public Blob getBlob(String boundary) {
			String fileContentType = CS_Utilities.getExtensionContentType(fileExtension);
			if (fileContentType == null) {
				throw new MultipartRequestException(
					'La extension solicitada (' +
					fileExtension +
					') no tiene un contentType asignado. Porfavor agregue el valor al a la clase CS_Utilities.'
				);
			}
			String fileHeader =
				'\r\n--' +
				boundary +
				'\r\nContent-Type: ' +
				+'\r\nContent-Disposition: form-data; name=\"' +
				key +
				'\"; filename=\"' +
				fileName +
				'.' +
				fileExtension +
				'\" \r\n\r\n';
			Blob fileHeaderBlob = Blob.valueOf(fileHeader);
			String fileFooter = '\r\n--' + boundary;
			Blob fileFooterBlob = Blob.valueOf(fileFooter);

			String combinedDataAsHex =
				EncodingUtil.convertToHex(fileHeaderBlob) +
				EncodingUtil.convertToHex(fileBlob) +
				EncodingUtil.convertToHex(fileFooterBlob);
			return EncodingUtil.convertFromHex(combinedDataAsHex);
		}
	}

	/**
	 * Constante con el separador usado en el cuerpo de las peticiones de servicios del tipo Multipart/Form-Data
	 */
	private static final String MULTIPART_BOUNDARY = '1ff13444ed8140c7a32fc4e6451aa76d';

	public String contentType { get; private set; }
	public CS_MultipartFile file { get; private set; }
	public List<CS_MultipartParameter> parameters { get; private set; }

	public CS_MultipartFormDataRequest() {
		contentType = 'multipart/form-data; charset="UTF-8"; boundary="' + MULTIPART_BOUNDARY + '"';
		parameters = new List<CS_MultipartParameter>();
	}

	public void addParameter(String key, String value) {
		CS_MultipartParameter newParameter = new CS_MultipartParameter(key, value);
		this.parameters.add(newParameter);
	}

	public void addFile(String key, Blob file, String fileName, String fileExtension) {
		this.file = new CS_MultipartFile(key, file, fileName, fileExtension);
	}

	public void addFile(String key, ContentVersion fileContentVersion) {
		this.file = new CS_MultipartFile(key, fileContentVersion.VersionData, fileContentVersion.Title, fileContentVersion.FileExtension);
	}

	public Blob getRequestBodyBlob() {
		String requestBodyString = '';
		for (CS_MultipartParameter parameter : parameters) {
			requestBodyString += parameter.getString(MULTIPART_BOUNDARY);
		}
		String combinedDataAsHex =
			EncodingUtil.convertToHex(file.getBlob(MULTIPART_BOUNDARY)) + EncodingUtil.convertToHex(Blob.valueOf(requestBodyString));
		return EncodingUtil.convertFromHex(combinedDataAsHex);
	}

