global with sharing class CS_SendCaseFilesSS_Invocable {
	@InvocableMethod(
		label='Smart Supervision Enviar Adjuntos Caso'
		description='EnvÃ­a los documentos adjuntos a un caso a la Superintendencia Financiera de Colombia'
		category='Case'
	)
	global static void sendAttachedFilesCase(List<Case> cases) {
		System.debug('--------------  CS_SmartSupervision_Invocable.sendAttachedFilesCase --------------');
		System.debug('1. CS_SmartSupervision_Invocable.sendAttachedFilesCase cases: ' + cases);

		//* Extraer variables
		String estadoCaso = cases.get(0).Status;
		System.debug('2. CS_SmartSupervision_Invocable.sendAttachedFilesCase estadoCaso: ' + estadoCaso);

		Set<Id> setIdCasos = new Set<Id>();
		for (Case caseObj : cases) {
			setIdCasos.add(caseObj.Id);
		}
		Map<Id, CS_DocumentoCaso__c> mapDocumentosCaso = new Map<Id, CS_DocumentoCaso__c>(
			[
				SELECT Id, CS_Caso__c
				FROM CS_DocumentoCaso__c
				WHERE
					CS_Caso__c IN :setIdCasos
					AND CS_MomentoEnvio__c = :estadoCaso
					AND CS_EstadoSincronizacionSuperfinanciera__c = 'No enviado'
			]
		);
		if (!mapDocumentosCaso.isEmpty()) {
			Map<Id, List<CS_DocumentoCaso__c>> mapIdCaseListDocumentosCasos = new Map<Id, List<CS_DocumentoCaso__c>>();
			for (CS_DocumentoCaso__c documentoCaso : mapDocumentosCaso.values()) {
				if (!mapIdCaseListDocumentosCasos.containsKey(documentoCaso.CS_Caso__c)) {
					mapIdCaseListDocumentosCasos.put(documentoCaso.CS_Caso__c, new List<CS_DocumentoCaso__c>());
				}
				mapIdCaseListDocumentosCasos.get(documentoCaso.CS_Caso__c).add(documentoCaso);
			}
			System.debug(
				'3. CS_SmartSupervision_Invocable.sendAttachedFilesCase mapIdCaseListDocumentosCasos: ' + mapIdCaseListDocumentosCasos
			);

			List<ContentDocumentLink> fileLinks = [
				SELECT Id, LinkedEntityId, ContentDocumentId
				FROM ContentDocumentLink
				WHERE LinkedEntityId IN :mapDocumentosCaso.keySet()
			];
			System.debug('4. CS_SmartSupervision_Invocable.sendAttachedFilesCase fileLinks: ' + fileLinks);
			Map<Id, Id> mapIdDocumentoCasoIdContentDocument = new Map<Id, Id>();
			for (ContentDocumentLink fileLink : fileLinks) {
				mapIdDocumentoCasoIdContentDocument.put(fileLink.LinkedEntityId, fileLink.ContentDocumentId);
			}
			System.debug(
				'5. CS_SmartSupervision_Invocable.sendAttachedFilesCase mapIdDocumentoCasoIdContentDocument: ' +
				mapIdDocumentoCasoIdContentDocument
			);

			for (Case caseObj : cases) {
				for (CS_DocumentoCaso__c documentoCaso : mapIdCaseListDocumentosCasos.get(caseObj.Id)) {
					CS_SmartSupervision_Future.addFileFuture(
						caseObj.Id,
						mapIdDocumentoCasoIdContentDocument.get(documentoCaso.Id),
						documentoCaso.Id
					);
				}
			}
		}
	}

